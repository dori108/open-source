#include <stdio.h>
#define MAX_STUDENTS 1000


int input_array(int a[], int n){
	/* Take input of scores into <a> for <n> students*/
	for(int i =0; i<n; i++){
		scanf("%d",&a[i]);
	}
}

int over(int a[],int num){
	int count =0;
	for(int i =0; i<num; i++){
		if (a[i]>= target){
			count +=1;
		}
	}
	return count;
}

void print(int arr[], int num){
	printf("%d",over(arr,num));
}

int main() {
	int arr[MAX_STUDENTS]={0};
	int num;
	int target;
	scanf("%d", &num);
	scanf("%d", &target);
	input_array(arr, num);
	print(arr,num);
	
}


